{
	"logo": "logo.png",
    "preload": "preload.js",
	"features": [
		{
		  "code": "send",
		  "explain": "send message to device",
		  "cmds":[{
            "type": "over",
            "label": "send",
            "minLength": 1
      }]
		},
    {
      "code": "setKey",
		  "explain": "set the device key",
		  "cmds":[{
            "type": "regex",
            "label": "setKey",
            "match": "/(https://api.day.app/.*)/"
      }]
    },
    {
      "code": "delKey",
		  "explain": "delete the key",
		  "cmds":["delKey"]
    },
    {
      "code": "showKey",
      "explain": "show the device key",
      "cmds": ["showKey"]
    }
	]
}